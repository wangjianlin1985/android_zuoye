<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="LeaveClass" table="LeaveClass">

          <!-- 主键请假类别id -->
          <id name="leaveClassId" column="leaveClassId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 请假类别名称 -->
          <property
            name="leaveClassName"
            column="leaveClassName"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
